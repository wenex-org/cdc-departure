{
  "name": "cdc-departure",
  "version": "0.0.1",
  "description": "CDC Departure",
  "author": "Vahid V. <vhid.vz@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/sql-to-nosql/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/sql-to-nosql/test/jest-e2e.json",
    "command": "node -r tsconfig-paths/register -r ts-node/register libs/command/src/main.ts",
    "command:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register libs/command/src/main.ts",
    "db:clean": "npm run command -- db clean",
    "db:seed": "npm run command -- db seed",
    "db:debug": "npm run command:debug -- db",
    "mysql:drop": "npm run command -- mysql drop",
    "mysql:seed": "npm run command -- mysql seed",
    "mysql:debug": "npm run command:debug -- mysql"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.2.6",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@typegoose/typegoose": "^11.5.0",
    "@willsoto/nestjs-prometheus": "^5.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "kafkajs": "^2.2.4",
    "log": "^6.3.1",
    "log-node": "^8.0.3",
    "mongodb": "^6.1.0",
    "mongoose": "^7.5.3",
    "mysql2": "^3.6.1",
    "naming-conventions-modeler": "^1.4.1",
    "nest-commander": "^3.12.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/log": "^6.3.1",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "^18.15.0",
    "npm": "^9.5.0"
  }
}
